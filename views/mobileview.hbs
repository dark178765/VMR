        .sidenav {
            height: 100%;
            width: 0;
            position: fixed;
            z-index: 1;
            top: 0;
            left: 0;
            background-color: #111;
            overflow-x: hidden;
            transition: 0.5s;
            padding-top: 60px;
        }

        .sidenav a {
            padding: 8px 8px 8px 32px;
            text-decoration: none;
            font-size: 25px;
            color: #818181;
            display: block;
            transition: 0.3s;
        }

        .sidenav a:hover {
            color: #f1f1f1;
        }

        .sidenav .closebtn {
            position: absolute;
            top: 0;
            right: 25px;
            font-size: 36px;
            margin-left: 50px;
        }

        #mySidenav a {
            color: #fff;
        }

        .button-mobile {
            border: 1px solid #dfdfdf;
            padding: 10px;
            border-radius: 5px;
            text-decoration: none;
            background: #FF875A;
            color: #fff;
            font-weight: bold;
            width: 50%;
            text-align: center;
            margin-right: 5px;
            display: block;
        }

        .button-primary-mobile {
            background: #e74765;
            color: #fff;
            border-color: #e74765;
        }

        @media screen and (max-height: 450px) {
            .sidenav {
                padding-top: 15px;
            }

            .sidenav a {
                font-size: 18px;
            }
        }

        .no-list li {
            list-style-type: none;

        }

        .no-list li a {
            color: #fff;
            text-decoration: none;
        }

        .center-video iframe,
        .center-video object,
        .center-video embed {
            /*position: absolute;*/
            top: 0;
            left: 0;
            width: 100%;
        }

        .center-video {
            text-align: center;
        }
    </style>


