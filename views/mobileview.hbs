<!DOCTYPE html>
<html lang="en">

<head>
    <title>{{metatitle}}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="{{metadescription}}" />
    <meta name="keywords" content="{{metakeywords}}" />
    <meta name="author" content="Vantage Market Research,{{host}}/">
    <meta name="copyright" content="Vantage Market Research" />
    <meta content="US" name="geo.country" />

    <link rel="preconnect" href="{{host}}/" />
    <link rel="preconnect" href="https://cdn.vantagemarketresearch.com">

    <link rel="dns-prefetch" href="{{host}}/" />

    <meta name="distribution" content="global" />
    <meta name="coverage" content="global" />
    <meta name="abstract" content="consulting services, market research reports, industry analysis, reports" />
    <meta name="Audience" content="All, Business, Management, Research, Services" />

    <!-- Twitter -->
    <meta name="twitter:card" content="summary_large_image" />
    <meta name="twitter:site" content="@vantagemarketr" />
    <meta name="twitter:creator" content="@vantagemarketr" />
    <meta property="twitter:description" content="{{metadescription}}">

    <!-- Open Graph / Facebook -->
    <meta property="og:site_name" content="Vantage Market Research" />

    <meta property="og:title" content="{{metatitle}}" />
    <meta property="og:description" content="{{metadescription}}" />
    <meta property="og:image" content="{{host}}/assets/images/logo/logo.webp" />
    <meta property="og:url" content="{{host}}">

    <link rel="canonical"
        href="{{#if_eq canonical undefined}}{{CURRENT_URL}}{{/if_eq}}{{#if_not_eq canonical undefined}}{{host}}{{canonical}}{{/if_not_eq}}" />
    <link href="/favicon.ico" rel="icon">
</head>

<body style="margin:0px;">
