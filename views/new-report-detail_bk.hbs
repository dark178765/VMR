<script>
    let savedFormValues = false;
    let leavingMessageShown = false;
    let message = 'We will be happy to help you!! Please provide your requirement. We will send you the sample as per your requirement.';
    $(document).ready(function () {

        setTimeout(() => {
            //if (!cookie()) {
            if (!savedFormValues) {
                $('.modal').modal('show');
                $('.popupMessage').text(message);
            }
            //}
        }, 15000);

        $('.market-table').addClass('table table-bordered even-table');
        $('.market-table').removeClass('market-table');
        $('html').bind('mouseleave', function () {
            if (!leavingMessageShown && !savedFormValues) {
                $('.popupMessage').text('Please let us know what you did not find in this report. That will give us the oppurtunity to improve. We insure you that you will get the exact information you want.');
                $('.modal').modal('show');
                leavingMessageShown = true;
            }
        });

        $(window).scroll(function () {
            var y = $(this).scrollTop();
            if (y > $('.tab-buttons').offset().top) {
                $('.replaceHeader').css('display', '');
            } else {
                $('.replaceHeader').css('display', 'none');
            }
        });
    });

    function cookie() {
        if (document.cookie.indexOf('showDialog=true;') == -1) {
            document.cookie = "showDialog=true;";
            return false;
        }
        return true;
    }
</script>

