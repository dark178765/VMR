<link href="/css/report-detail.css" rel="stylesheet" />
<script src="https://cdn.jsdelivr.net/npm/chart.js@2.9.4/dist/Chart.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/chartjs-plugin-deferred"></script>

<main id="report-details-main">
    <div class="container-fluid">

        <div class="row">
            <div class="col-md-12 blog-post-inner">
                <div class="card mb-4">
                    <div class="main-title-section">

                        <div class="img-section">
                            <div class="report-title">
                                <h1 class="card-title">{{keyword}} Market</h1>
                                <div class="card-body">
                                    <h4 class="card-text">
                                        {{title}}
                                    </h4>
                                </div>
                                <div class="card-footer  row">
                                    <div
                                        class="post-date-and-category col-md-9 col-sm-12 col-lg-9 col-xxl-9 col-xs-12 col-12">
                                        <ul>
                                            <li>
                                                <i class="fa fa-calendar"> </i>
                                                <time class="entry-date published updated"
                                                    datetime="{{actualCreatedAt}}">
                                                    {{createdAt}} | Base Year:
                                                    {{baseYear}} |
                                                    Number of Pages:
                                                    {{numberOfPages}}</time>
                                                | Region : Global | Format:
                                                <i class="fa fa-file-pdf-o" style="color: red;" title="PDF"></i> <i
                                                    class="fa fa-file-excel-o" style="color: green;" title="Excel"></i>
                                                <i class="fa fa-file-powerpoint-o" style="color: blue;"
                                                    title="Power Point Presentation (PPT)"></i> | Report ID:
                                                {{reportId}}
                                            </li>

                                        </ul>
                                        <div class="pt-4">
                                            <ul>
                                                <li class="mr-0">
                                                    <span>Home</span>&nbsp;<i class="fa fa-angle-double-right"></i>
                                                </li>
                                                <li class="mr-0">
                                                    <span>{{category}}</span>&nbsp;<i
                                                        class="fa fa-angle-double-right"></i>
                                                </li>
                                                <li class="mr-0">
                                                    <span>{{keyword}} Market</span>
                                                </li>
                                            </ul>

                                        </div>
                                    </div>
                                    <div class="col-md-3 col-sm-12 col-lg-3 col-xxl-3 col-xs-12 col-12">
                                        <ul class="social-bookmark float-end p-0">
                                            <li><a class="facebook"><i class="fa fa-facebook"></i></a></li>
                                            <li><a class="twitter"><i class="fa fa-twitter"></i></a>
                                            </li>
                                            <li><a class="linkedin"><i class="fa fa-linkedin"></i></a></li>
                                            <li><a class="linkedin"><i class=" fa fa-envelope"></i></a></li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="col-md-12 col-sm-12 col-lg-9 col-xxl-9 col-xs-12 col-12 blog-post-inner" id="right_win">
                <div class="container nav-tabs-main p-0">
                    <div class="row">
                        <div class="col-md-12 ml-auto col-xl-12 mr-auto">
                            <div class="card">
                                <div class="card-header">
                                    <ul class="nav nav-tabs nav-tabs-neutral justify-content-center tab-buttons"
                                        role="tablist" data-background-color="orange">
                                        <li class="nav-item">
                                            <a class="btn btn-dark{{#if_eq descriptionTab true}} active{{/if_eq}}"
                                                href="/industry-report/{{slug}}" role="tab">Description</a>
                                        </li>
                                        <li class="nav-item">
                                            <a class="btn btn-light ml-2{{#if_eq tocTab true}} active{{/if_eq}}"
                                                href="/toc/{{slug}}" role="tab">Table of
                                                Content</a>
                                        </li>
                                        <li class="nav-item">
                                            <a class="btn btn-warning ml-2{{#if_eq marketSegmentTab true}} active{{/if_eq}}"
                                                href="/market-segmentation/{{slug}}" routerLinkActive="active"
                                                role="tab">
                                                Market
                                                Segmentation</a>
                                        </li>
                                        <li class="nav-item">
                                            <a class="btn btn-success ml-2{{#if_eq methodologyTab true}} active{{/if_eq}}"
                                                href="/methodology/{{slug}}" role="tab">Methodology</a>
                                        </li>
                                    </ul>
                                </div>

                                <div class="card-body tab-contant-section">
                                    <div class="tab-content p-4">
                                        <div id="report_description"
                                            class="tab-pane{{#if_eq descriptionTab true}} active{{/if_eq}}" id="home1"
                                            role="tabpanel" id="description-section">
                                            <h3 class="summary-table-title">Market Snapshot</h3>
                                            {{{summaryTable}}}
                                            {{{reportDescription}}}

                                            {{#if pr}}
                                            <!-- CTA -->
                                            <div class="gradient-overlay-half-primary-v1 cta"
                                                style="padding: 22px 0px !important;">
                                                <div class="bg-img-hero">
                                                    <div class="container">
                                                        <div
                                                            class="row align-items-lg-center text-center text-lg-left space-2">
                                                            <div class="col-md-12">
                                                                <h2 class="text-white">{{pr.title}}</h2>
                                                            </div>
                                                            <div class="col-lg-7">
                                                                <p class="lead text-white mb-0">{{substring
                                                                    pr.description 0 250}}</p>
                                                            </div>

                                                            <div class="col-lg-5 text-lg-right">
                                                                <a class="btn btn-dark mb-2 mb-sm-0 mr-sm-2"
                                                                    href="/{{slug}}/request-sample">
                                                                    Request Sample</a>
                                                                <a class="btn btn-light mb-2 mb-sm-0"
                                                                    href="/press-release/{{pr.slug}}">Learn More</a>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            {{/if}}
                                            <!-- End CTA -->
                                        </div>

                                        <div class="tab-pane fade show{{#if_eq tocTab true}} active{{/if_eq}}"
                                            id="profile1" role="tabpanel">
                                            {{#if_eq tocTab true}}
                                            <div>{{{toc}}}</div>
                                            {{/if_eq}}
                                        </div>
                                        <div class="tab-pane fade show{{#if_eq marketSegmentTab true}} active{{/if_eq}}"
                                            role="tabpanel">
                                            {{#if_eq marketSegmentTab true}}
                                            {{> marketsegmentation marketSeg}}
                                            {{/if_eq}}
                                        </div>
                                        <div class="tab-pane fade show{{#if_eq methodologyTab true}} active{{/if_eq}}"
                                            role="tabpanel">
                                            {{#if_eq methodologyTab true}}
                                            {{> newmethodology methodology}}
                                            {{/if_eq}}
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <section id="faq" class="faq section-bg">
                    <div class="container">
                        <div class="section-title">
                            <h2>Frequently Asked Questions</h2>
                        </div>

                        <div class="faq-list">
                            <ul id="accordion1">
                                <li data-aos="fade-up" class="panel">
                                    <i class="fa fa-question-circle  icon-help"></i>
                                    <a data-bs-toggle="collapse" data-toggle="collapse" data-bs-target="#faq-list-1"
                                        href="#faq-list-1" data-parent="#accordion1">What is
                                        the global demand for {{keyword}}
                                        in terms of revenue? <i class="fa fa-angle-down icon-show"></i>
                                        <i class="fa fa-angle-up icon-close"></i>
                                    </a>
                                    <div id="faq-list-1" class="collapse" data-bs-parent=".faq-list">
                                        <p class="p-4">
                                            The global {{keyword}} valued at {{Revenue_2021}}
                                            in 2020 and is expected to reach {{Revenue_2028}} in 2028
                                            growing at a CAGR of {{CAGR_Revenue}}
                                        </p>
                                    </div>
                                </li>

                                <li data-aos="fade-up" data-aos-delay="100">
                                    <i class="fa fa-question-circle  icon-help"></i>
                                    <a data-bs-toggle="collapse" data-bs-target="#faq-list-2" data-toggle="collapse"
                                        href="#faq-list-2" data-parent="#accordion1"> Which
                                        are the prominent players
                                        in the market? <i class="fa fa-angle-down icon-show"></i><i
                                            class="fa fa-angle-up icon-close"></i></a>
                                    <div id="faq-list-2" class="collapse" data-bs-parent=".faq-list">
                                        <p class="p-4">
                                            The prominent players in the market are {{Players}}.
                                        </p>
                                    </div>
                                </li>

                                <li data-aos="fade-up" data-aos-delay="200">
                                    <i class="fa fa-question-circle  icon-help"></i> <a data-bs-toggle="collapse"
                                        data-bs-target="#faq-list-3" class="collapse" data-toggle="collapse"
                                        href="#faq-list-3" data-parent="#accordion1">
                                        At what CAGR is the market projected to grow within the forecast period? <i
                                            class="fa fa-angle-down icon-show"></i><i
                                            class="fa fa-angle-up icon-close"></i></a>
                                    <div id="faq-list-3" class="collapse" data-bs-parent=".faq-list">
                                        <p class="p-4">
                                            The market is project to grow at a CAGR of
                                            {{CAGR_Revenue}}
                                            between 2021 and 2028.
                                        </p>
                                    </div>
                                </li>

                                <li data-aos="fade-up" data-aos-delay="300">
                                    <i class="fa fa-question-circle  icon-help"></i>
                                    <a data-bs-toggle="collapse" data-bs-target="#faq-list-4" class="collapse"
                                        data-toggle="collapse" href="#faq-list-4" data-parent="#accordion1">What are
                                        the driving factors
                                        fueling the growth of the market. <i class="fa fa-angle-down icon-show"></i><i
                                            class="fa fa-angle-up icon-close"></i></a>
                                    <div id="faq-list-4" class="collapse" data-bs-parent=".faq-list">
                                        <p class="p-4">
                                            The driving factors of the {{keyword}} include
                                        <ul style="list-style: disc;" class="pl-lg-5 text-justify">
                                            <li class="pl-0 pt-0 pb-0">
                                                {{driver_1}}
                                            </li>
                                            {{#if drover_2}}
                                            <li class="pl-0 pt-0 pb-0">
                                                {{driver_2}}
                                            </li>
                                            {{/if}}
                                        </ul>
                                        </p>
                                    </div>
                                </li>

                                <li data-aos="fade-up" data-aos-delay="400">
                                    <i class="fa fa-question-circle  icon-help"></i>
                                    <a data-bs-toggle="collapse" data-bs-target="#faq-list-5" class="collapse"
                                        data-toggle="collapse" href="#faq-list-5" data-parent="#accordion1">Which
                                        region accounted for the
                                        largest share in the market? <i class="fa fa-angle-down icon-show"></i><i
                                            class="fa fa-angle-up icon-close"></i></a>
                                    <div id="faq-list-5" class="collapse" data-bs-parent=".faq-list">
                                        <p class="p-4">
                                            {{Largest_Region}} was the leading regional segment of
                                            the
                                            {{keyword}} in 2020.
                                        </p>
                                    </div>
                                </li>

                            </ul>
                        </div>

                    </div>
                </section>

            </div>

