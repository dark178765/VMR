                        {{#script_section 'GPT_Script'}}
                        <script src="https://unpkg.com/typed.js@2.0.16/dist/typed.umd.js"></script>
                        <script>

                            var typed = null

                            let fetching = false;

                            function fetchResponse() {
                                fetching = true;
                                $('#chatfooter').css('display', 'none');

                                $('#chatResponse').html();
                                $('#loader').show();

                                let urlSegment = window.location.href.split('/');
                                let url = urlSegment[urlSegment.length - 1];
                                $.ajax({
                                    type: 'POST',
                                    url: '/bot-chat',
                                    data: JSON.stringify({
                                        question: $('.chat-input').val(),
                                        url: url.split('?')[0]
                                    }),
                                    contentType: 'application/json; charset=utf-8',
                                    success: function (res) {
                                        //$('#chatResponse').html(res);
                                        $('#loader').hide();
                                        if (typed)
                                            typed.destroy();

                                        if (res == null || res === '' || res.generatedText === '') {
                                            typed = new Typed('#chatResponse', {
                                                strings: ['Sorry!!. You exhausted your attempts!']
                                            });
                                        } else {
                                            document.cookie = 'chatID='
                                            typed = new Typed('#chatResponse', {
                                                strings: [res.generatedText.replace(/&/gmi, '&amp;')]
                                            });
                                        }

                                        //setCookie('chatID', res.id, 30);

                                        $('#chatID').val(res.id)
                                        $('#chatfooter').css('display', '');
                                        fetching = false;
                                    },
                                    error: function (err) {
                                        console.log(err);
                                        fetching = false;
                                    }
                                })
                            }

                            let originalTitle = '';

                            function showModal() {
                                $('#popupFormType').val(10);
                                originalTitle = $('.modal-title').text();
                                $('.modal-title').text('Get more information');

                                var myModal = new bootstrap.Modal(document.getElementById('myModal'));
                                myModal.show();

                                let myModalEl = document.getElementById('myModal');

                                myModalEl.addEventListener('hidden.bs.modal', function (event) {
                                    myModalEl.removeEventListener('hidden.bs.modal', () => { });
                                    $('.modal-title').text(originalTitle);
                                    $('#popupFormType').val("");
                                });

                            }

                            function loadMore() {
                                let urlSeg = window.location.href.split('/');
                                $('#loader').show();
                                $('#chatfooter').hide();
                                $.ajax({
                                    type: 'POST',
                                    url: '/load-more',
                                    contentType: 'application/json; charset=utf-8',
                                    data: JSON.stringify({
                                        id: $('#leadID').val(),
                                        chatId: $('#chatID').val(),
                                        url: urlSeg[urlSeg.length - 1]
                                    }),
                                    success: function (res) {
                                        $('#loader').hide();
                                        let existingHML = $('#chatResponse').html();

                                        for (var i = 0; i < res.length; i++) {
                                            existingHML += '<br/><br/>'
                                                + res[i].generatedText.replace(/&/gmi, '&amp;');
                                        }

                                        $('#chatResponse').html(existingHML);
                                        $('#chatfooter').hide();
                                        $('.dummy-contnet').hide();
                                    },
                                    error: function (err) {
                                        console.log(err)
                                    }
                                })
                            }

                            $(document).ready(function () {
                                $('.chat-input').keyup(function (e) {
                                    if (e.keyCode == 13 && !fetching) {
                                        fetchResponse();
                                    }
                                })
                            });

                            //cookie thing
                            function setCookie(name, value, days) {
                                var expires = "";
                                if (days) {
                                    var date = new Date();
                                    date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
                                    expires = "; expires=" + date.toUTCString();
                                }
                                document.cookie = name + "=" + (value || "") + expires + "; path=/";
                            }
                            function getCookie(name) {
                                var nameEQ = name + "=";
                                var ca = document.cookie.split(';');
                                for (var i = 0; i < ca.length; i++) {
                                    var c = ca[i];
                                    while (c.charAt(0) == ' ') c = c.substring(1, c.length);
                                    if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length, c.length);
                                }
                                return null;
                            }
                            function eraseCookie(name) {
                                document.cookie = name + '=; Path=/; Expires=Thu, 01 Jan 1970 00:00:01 GMT;';
                            }
                        </script>
