    document.addEventListener('scroll', (e) => {
        let chartSection = document.getElementsByClassName('segmenttree');

        if (chartSection.length == 0) {
            chartSection = document.getElementsByClassName('dialog-show');
        }

        var y = document.documentElement.scrollTop;

        if (chartSection && chartSection.length > 0) {
            if (y > chartSection[0].offsetTop + 2000) {
                if (!shownDialog)
                    myModal.show();
                shownDialog = true;
            }
        }

        let tabbuttons = document.getElementsByClassName('tab-buttons');
        if (y > tabbuttons[0].offsetTop) {
            //$('.replaceHeader').css('display', '');
            document.getElementsByClassName('replaceHeader')[0].style = 'display: block;';
        } else {
            document.getElementsByClassName('replaceHeader')[0].style = 'display: none;';
        }

        let trustImages = document.getElementById('tr-report');
        let letop = document.getElementsByClassName('le');

        if (y > trustImages.offsetTop + 900) {
            if (y < letop[0].offsetTop - 500) {
                document.getElementsByClassName('buynow-container')[0].style = 'position: fixed; top: 85px; right: 20px;';
                //$('.buynow-container').theiaStickySidebar();
            } else {
                document.getElementsByClassName('buynow-container')[0].style = 'position: sticky; top: 85px; right: 20px;';
            }
        } else {
            document.getElementsByClassName('buynow-container')[0].style = '';
        }
    })

    $(document).ready(function () {
        $('.fa-lock').css('cursor', 'pointer');
        $('.fa-lock').click(function () {
            myModal.show();
        });
    })
</script>
