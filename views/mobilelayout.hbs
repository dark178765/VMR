    {{>newsearch}}

    {{>reffootermobile}}


    {{#each scriptsection}}
    {{{.}}}
    {{/each}}

    <script>
        $(document).ready(function () {
            $(`.main-nav li`).removeClass('active');
            $(`.main-nav a[href='${window.location.pathname}']`).parent().addClass('active');
        })
    </script>



    <script defer src="https://cdn.vantagemarketresearch.com/assets/js/script.js"></script>


    {{>cookieconsent}}

    <script src="/js/lazysizes.min.js"></script>
